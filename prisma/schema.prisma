generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  edad     Int
  nombre   String
  email    String @unique
  role     Role   @default(USUARIO)
  id       Int    @id @default(autoincrement())
  password String

  @@unique([edad, nombre])
  @@index([email])
}

enum Role {
  USUARIO
  ADMIN
}
